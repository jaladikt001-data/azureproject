{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifymusicfactory"
		},
		"Azure_SqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_SqlDatabase'"
		},
		"datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake'"
		},
		"Azure_SqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ktspotify123.database.windows.net"
		},
		"Azure_SqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifysql"
		},
		"Azure_SqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ktadmin"
		},
		"datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifydataproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifymusicfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_SqlDatabase_password": {
						"value": "[parameters('Azure_SqlDatabase_password')]"
					},
					"datalake_accountKey": {
						"value": "[parameters('datalake_accountKey')]"
					},
					"Azure_SqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('Azure_SqlDatabase_properties_typeProperties_server')]"
					},
					"Azure_SqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('Azure_SqlDatabase_properties_typeProperties_database')]"
					},
					"Azure_SqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('Azure_SqlDatabase_properties_typeProperties_userName')]"
					},
					"datalake_properties_typeProperties_url": {
						"value": "[parameters('datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}